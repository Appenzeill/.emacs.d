;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(extended-command-history read-expression-history file-name-history))
(setq extended-command-history '("restart-emacs"))
(setq read-expression-history '("wq"))
(setq file-name-history '("~/.emacs.d/general.org"))
